doctype html
block vars
  - var bodyclass = null;
html(lang='en')
  head
    block head
      meta(charset='utf-8')
      meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
      meta(name='viewport', content='width=device-width')
      title
        block title
          = locals.name

      link(rel='alternate', href=locals.url+'/feed.xml', type='application/rss+xml', title=locals.description)
      // Bootstrap
      link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css", integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7", crossorigin="anonymous")

      //     Stylesheets
      link(href=contents.css['ct-paper.css'].url, rel="stylesheet")

      //     Fonts and icons
      link(href='http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css', rel="stylesheet")
      link(href='https://fonts.googleapis.com/css?family=Fira+Sans', rel='stylesheet', type='text/css')
  body
    div.navbar.navbar-relative#register-navbar(role='navigation')
      div.container
        div.navbar-header.navbar-right
          button.navbar-toggle(data-toggle="collapse", data-target=".navbar-collapse", type="button")
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar

        block nav
    hr.hidden-xs
    div.wrapper
      div.container
        div.row
          div.col-lg-8.col-lg-offset-2.col-md-10.col-md-offset-1
          block content
    script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js")
    script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js", integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS", crossorigin="anonymous")
